if game.PlaceId == 10449761463 then
   local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
   
   -- Variables to store slider values
   local walkSpeedValue = 16
   local jumpPowerValue = 50

   -- Function to update character stats
   local function updateCharacterStats()
      local player = game.Players.LocalPlayer
      if player and player.Character and player.Character:FindFirstChild("Humanoid") then
         player.Character.Humanoid.WalkSpeed = walkSpeedValue
         player.Character.Humanoid.JumpPower = jumpPowerValue
      end
   end

   local Window = Rayfield:CreateWindow({
      Name = "Arsenic Hub",
      Icon = 0, 
      LoadingTitle = "Arsenic Hub Is Loading",
      LoadingSubtitle = "by JacKScripterz",
      Theme = "Default",
      DisableRayfieldPrompts = false,
      DisableBuildWarnings = false, 
      ConfigurationSaving = {
         Enabled = true,
         FolderName = "ArsenicHub", 
         FileName = "Config"
      },
      Discord = {
         Enabled = false,
         Invite = "noinvitelink", 
         RememberJoins = true 
      },
      KeySystem = false, 
      KeySettings = {
         Title = "Untitled",
         Subtitle = "Key System",
         Note = "No method of obtaining the key is provided", 
         FileName = "Key", 
         SaveKey = true, 
         GrabKeyFromSite = false, 
         Key = {"Hello"} 
      }
   })
   
   local MainTab = Window:CreateTab("üè†Home", nil)
   local MainSection = MainTab:CreateSection("Custom Movesets")
   
   -- Connect character added event
   game.Players.LocalPlayer.CharacterAdded:Connect(updateCharacterStats)
   
   Rayfield:Notify({
      Title = "Made By JacKScripterz",
      Content = "Welcome to Arsenic Hub",
      Duration = 5,
      Image = nil
   })
   
   -- Your existing buttons remain the same...
   
   local MainSection2 = MainTab:CreateSection("Miscellaneous")

   -- Modified sliders with error handling
   local WalkSpeedSlider = MainSection2:CreateSlider({
      Name = "Walk Speed",
      Range = {16, 100},
      Increment = 1,
      Suffix = "Speed",
      CurrentValue = walkSpeedValue,
      Flag = "WalkSpeedSlider",
      Callback = function(Value)
         walkSpeedValue = Value
         pcall(function()
            updateCharacterStats()
         end)
      end
   })

   local JumpPowerSlider = MainSection2:CreateSlider({
      Name = "Jump Power",
      Range = {50, 200},
      Increment = 1,
      Suffix = "Power",
      CurrentValue = jumpPowerValue,
      Flag = "JumpPowerSlider",
      Callback = function(Value)
         jumpPowerValue = Value
         pcall(function()
            updateCharacterStats()
         end)
      end
   })

   -- Update initial character stats
   task.spawn(function()
      wait(1) -- Wait for character to load
      updateCharacterStats()
   end)

   Rayfield:LoadConfiguration()
end
